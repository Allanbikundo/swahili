{
    "name": "swahili",
    "displayName": "Swahili Syntax Highlighter",
    "description": "Syntax Highlighter for swahili",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [{
            "id": "swahili",
            "aliases": ["swahili", "swahili"],
            "extensions": [".swh"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "swahili",
            "scopeName": "source.swh",
            "path": "./syntaxes/swahili.tmLanguage.json"
        }]
    }
}